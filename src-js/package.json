  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
    "prettify": "prettier --config .prettierrc.js 'src/**/*.{js,ts,tsx}' --write",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
